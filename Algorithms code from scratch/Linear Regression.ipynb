   "source": [
    "def linear_regression_closed_form(X, y):\n",
    "    n = X.shape[0]\n",
    "    A = 1/n * X.T @ X \n",
    "    b = 1/n * X.T @ y \n",
    "    theta = np.linalg.inv(A) @ b\n",
    "    return theta "
   ]
